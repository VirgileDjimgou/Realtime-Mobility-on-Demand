<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bee.passenger">

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CAMERA"/>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE" />

    <!-- Used for Importing / Exporting Wallets -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <!-- Used for QR Code scanning -->
    <uses-permission android:name="android.permission.CAMERA" />

    <!-- Used for optional Applock Feature -->
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-feature android:name="android.hardware.camera" android:required="false" />
    <uses-feature android:name="android.hardware.fingerprint" android:required="false" />
    <uses-feature android:name="android.hardware.camera.autofocus" android:required="false" />
    <uses-feature android:name="android.hardware.telephony" android:required="false" />
    <uses-feature android:name="android.hardware.location" android:required="false" />
    <uses-feature android:name="android.hardware.location.gps" android:required="false" />
    <uses-feature android:name="android.hardware.location.network" android:required="false" />
    <uses-feature android:name="android.hardware.microphone" android:required="false" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <supports-screens android:anyDensity="true" />

    <application
        android:name="com.bee.passenger.promotions_swipe.MyApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_bee"
        android:roundIcon="@mipmap/ic_bee_round"
        android:supportsRtl="true"
        android:launchMode="singleTop"
        android:screenOrientation="portrait"
        android:resizeableActivity="true"
        android:theme="@style/AppTheme">
        <service android:name="com.bee.passenger.onAppKilled" android:stopWithTask="false"/>


        <!-- mULTIWINDOWS  -->

        <uses-library android:required="false" android:name="com.sec.android.app.multiwindow"/>

        <meta-data android:name="com.sec.android.support.multiwindow.penwindow.enable" android:value = "true" />
        <meta-data android:name="com.sec.android.support.multiwindow" android:value = "true" />
        <meta-data android:name="com.sec.android.multiwindow.DEFAULT_SIZE_W"
            android:resource="@dimen/app_defaulsize_w"/>
        <meta-data android:name="com.sec.android.multiwindow.DEFAULT_SIZE_H"
            android:resource="@dimen/app_defaulsize_h"/>
        <meta-data android:name="com.sec.android.multiwindow.MINIMUN_SIZE_W"
            android:resource="@dimen/app_minimunsize_w"/>
        <meta-data android:name="com.sec.android.multiwindow.MINIMUN_SIZE_H"
            android:resource="@dimen/app_minimunsize_h"/>


        <meta-data android:name="com.samsung.android.sdk.multiwindow.multiinstance.enable"
            android:value="true"/>


        <!-- End Multiwindows -->

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />



        <meta-data android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id"/>



        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/notification_channel_id" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@mipmap/ic_bee_round" />

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />

        <service
            android:name="com.bee.passenger.fcm.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name="com.bee.passenger.fcm.MyFirebaseInstanceIDService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>




        <activity
            android:name="com.bee.passenger.activity.MainActivity_App">
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <category android:name="android.intent.category.MULTIWINDOW_LAUNCHER" />
            </intent-filter>
        </activity>




        <activity
            android:name="com.bee.passenger.fcm.MainActivity_fcm"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/TranslucentBackground">
        </activity>


        <activity android:name="com.bee.passenger.activity.MainActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/TranslucentBackground">
        <intent-filter>
            <action android:name="Start_Main"/>
            <category android:name="android.intent.category.DEFAULT"/>
        </intent-filter>>
        </activity>


        <activity
            android:name="com.bee.passenger.activity.RegisterActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:theme="@style/Translucent" />

        <activity
            android:name="com.bee.passenger.activity.AboutUsActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:label="@string/nav_about" />

        <activity
            android:name="com.bee.passenger.activity.EmailLoginActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>


        <activity
            android:name="com.bee.passenger.activity.PhoneAuthActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.bee.passenger.activity.PhoneRegistrationActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>


        <activity android:name="com.bee.passenger.promotions_swipe.PromotionsActivity"
            android:theme="@style/AppTheme.NoActionBar"/>


        <activity
            android:name="com.bee.passenger.cardview.History_CardView"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>



        <activity
            android:name="com.bee.passenger.activity.PrivacyPolicyActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:label="@string/activity_title_privacy_policy" />


        <activity android:name="com.bee.passenger.activity.Facebook_Activity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:label="@string/app_name" />

        <activity android:name="com.bee.passenger.activity.SplaschScreen"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"/>

        <activity android:name="com.bee.passenger.CustomerSettingsActivity"/>
        <activity android:name="com.bee.passenger.HistorySingleActivity"/>
        <activity android:name="com.bee.passenger.HistoryActivity"/>


        <activity android:name="com.facebook.FacebookActivity"

            android:configChanges=
                "keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name" />



        <activity
            android:name="com.bee.passenger.airbnbmapexample.ui.activity.MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name="com.bee.passenger.airbnbmapexample.ui.activity.SearchPlaceOnMapActivity" />
        <activity android:name="com.bee.passenger.airbnbmapexample.ui.activity.ShowDirectionActivity" />
        <activity android:name="com.bee.passenger.airbnbmapexample.ui.activity.MovingMarkerActivity" />
        <activity android:name="com.bee.passenger.airbnbmapexample.ui.activity.ListViewActivity" />
        <activity android:name="com.bee.passenger.airbnbmapexample.ui.activity.RecyclerViewLiteModeMapActivity"></activity>



        <activity
            android:name="com.facebook.CustomTabActivity"
            android:launchMode="singleTop"
            android:screenOrientation="portrait"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data android:scheme="@string/facebook_app_id"/>
            </intent-filter>
        </activity>


    </application>

</manifest>